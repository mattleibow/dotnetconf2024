<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublishTrimmed>true</PublishTrimmed>
	</PropertyGroup>

	<ItemGroup>
		<RuntimeHostConfigurationOption Include="OptionalFeature" Value="false" />
		<!--<RuntimeHostConfigurationOption Include="OptionalFeature" Value="false" Trim="true" />-->
	</ItemGroup>

</Project>
